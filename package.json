    "jest-extended": "^3.2.0",
    "jest-joi": "^1.1.14",
    "joi": "^17.7.0",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "turbo": "^1.6.3",
    "typescript": "^4.9.3"
