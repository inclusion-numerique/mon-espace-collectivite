{
  "name": "mec",
  "description": "Mon espace collectivit√© - Monorepo",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "packageManager": "npm@8.19.0",
  "engines": {
    "node": "^19.1",
    "npm": "^8.19"
  },
  "workspaces": {
    "packages": [
      "apps/web",
      "packages/api",
      "packages/auth",
      "packages/db",
      "packages/email"
    ],
    "nohoist": [
      "**/web/**"
    ]
  },
  "scripts": {
    "build": "turbo build",
    "clean": "rm -rf node_modules",
    "clean:workspaces": "turbo clean",
    "db-generate": "turbo db-generate",
    "db-push": "turbo db-push",
    "dev": "turbo dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint && manypkg check",
    "manypkg": "manypkg",
    "eslint": "eslint",
    "tsc": "turbo tsc",
    "docker:build:web": "docker build -t mec:0.1.0 -f docker/web/Dockerfile .",
    "docker:build:web:debug": "DOCKER_BUILDKIT=0 docker build -t mec:0.1.0 -f docker/web/Dockerfile ."
  },
  "dependencies": {
    "@manypkg/cli": "^0.19.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.29.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-turbo": "^0.0.7",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-extended": "^3.2.0",
    "jest-joi": "^1.1.14",
    "joi": "^17.7.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "optionalDependencies": {
    "turbo-linux-arm64": "^1.6.3"
  }
}
