{
  "name": "mec",
  "description": "Mon espace collectivit√© - Monorepo",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "engines": {
    "node": "^19.1",
    "npm": "^8.19"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "nohoist": [
      "**/web/**"
    ]
  },
  "scripts": {
    "build": "turbo build",
    "clean": "rm -rf node_modules",
    "clean:workspaces": "turbo clean",
    "db-generate": "turbo db-generate",
    "db-push": "turbo db-push",
    "dev": "turbo dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint && manypkg check",
    "eslint": "eslint",
    "tsc": "turbo tsc",
    "start:web": "npm -w @mec/web start",
    "docker:build:web": "docker buildx build --platform linux/amd64 -t rg.fr-par.scw.cloud/mon-espace-collectivite/mec:latest -f docker/web/Dockerfile .",
    "docker:build:web:debug": "DOCKER_BUILDKIT=0 docker build -t rg.fr-par.scw.cloud/mon-espace-collectivite/mec:debug -f docker/web/Dockerfile ."
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-turbo": "^0.0.7",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-extended": "^3.2.0",
    "jest-joi": "^1.1.14",
    "joi": "^17.7.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "optionalDependencies": {
    "turbo-linux-arm64": "^1.6.3"
  }
}
