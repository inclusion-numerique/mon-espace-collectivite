{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "db-generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^db-generate"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^db-generate"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "tsc": {
      "dependsOn": ["^db-generate"],
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "PORT",
    "APP",
    "PRODUCTION_URL",
    "PRODUCTION_APP",
    "BASE_URL",
    "NEXT_PUBLIC_SIREN_API_KEY",
    "NEXT_PUBLIC_CONTACT_EMAIL",
    "SIREN_ACCESS_TOKEN",
    "SCALEWAY_SECRET_KEY",
    "SCALEWAY_ACCESS_KEY",
    "SCALEWAY_S3_BUCKET_LOCATION",
    "SCALEWAY_BUCKET_ID",
    "SCALEWAY_S3_HOST",
    "GRIST_TABLE_ID",
    "GRIST_DOCUMENT_ID",
    "GRIST_API_KEY",
    "EMAIL_FROM_ADDRESS",
    "EMAIL_FROM_NAME",
    "EMAIL_SERVER",
    "SCALEWAY_SECRET_KEY",
    "SCALEWAY_SECRET_KEY"
  ]
}
