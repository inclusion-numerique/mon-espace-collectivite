{
  "name": "@mec/web",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "scalingo-postbuild": "next build && modclean -Pr && npm prune --omit=dev",
    "modclean": "modclean -Pr",
    "modclean:root": "modclean -Pr -p ./../..",
    "start": "next start",
    "lint": "next lint",
    "tsc": "tsc --noEmit",
    "postinstall": "npm run setup:dsfr",
    "setup:dsfr": "FROM=\"../../node_modules/@gouvfr/dsfr/dist\" TO=\"public/dsfr\" && mkdir -p public/dsfr/utility && cp $FROM/dsfr.module.min.js $TO/ && cp $FROM/dsfr.nomodule.min.js $TO/ && cp $FROM/dsfr.min.css $TO/ && cp $FROM/utility/utility.min.css $TO/utility/ && cp -R $FROM/icons $TO/ && cp -R $FROM/artwork $TO/ && cp -R $FROM/fonts $TO/",
    "test": "jest",
    "with-env": "dotenv -e ../../.env --",
    "prisma:dev": "npm with-env prisma studio --port 5556",
    "prisma:db:push": "npm run with-env prisma db push",
    "prisma:migrate:dev": "npm run with-env prisma migrate dev",
    "prisma:generate": "npm run with-env prisma generate",
    "prisma:format": "npm run with-env prisma format"
  },
  "engines": {
    "node": ">=19",
    "npm": ">=8"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.229.0",
