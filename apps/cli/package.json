{
  "name": "@mec/cli",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "tsc": "tsc --noEmit",
    "build": "tsc",
    "with-env": "dotenv -e ../../.env --",
    "admin-data:download:epci": "npm run with-env ts-node src/downloadEpcisData.ts",
    "admin-data:download:municipalities": "npm run with-env ts-node src/downloadMunicipalitiesData.ts",
    "admin-data:update":  "npm run with-env ts-node src/updateAdministrativeData.ts",
    "admin-data:setup": "npm run admin-data:download:epci && npm run admin-data:download:municipalities && npm run admin-data:update"
  },
  "engines": {
    "node": ">=19",
    "npm": ">=8"
  },
  "dependencies": {
    "@mec/web": "*",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.12",
    "typescript": "^4.9.4"
  }
}
